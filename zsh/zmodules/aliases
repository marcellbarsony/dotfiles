alias c='clear'
alias l='ls -laF --color=auto'
alias ls='ls --color=auto'
alias mv='mv -i'
alias rm='rm -i'


# Navigation
alias ..='cd ..'

# NeoVim
alias v='nvim'
alias nv='nvim'
alias vim='nvim'

# Git
alias g='git'
alias gs='git status'
alias ga='git add'
alias gp='git push'
alias gc='git commit -m'
