  "files.restoreUndoStack": true,
  "files.saveConflictResolution": "askUser",
  "files.simpleDialog.enable": false,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
