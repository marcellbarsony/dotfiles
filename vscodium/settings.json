// Files
  "files.associations": {},
  "files.autoGuessEncoding": false,
  "files.autoSave": "off",
  "files.autoSaveDelay": 1000,
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.enableTrash": false,
  "files.encoding": "utf8",
  "files.eol": "auto",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
